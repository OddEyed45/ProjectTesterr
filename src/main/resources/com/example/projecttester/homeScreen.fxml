<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projecttester.HelloController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../FINALHOME.png" />
         </image>
      </ImageView>
      <Button fx:id="race1" layoutX="109.0" layoutY="21.0" mnemonicParsing="false" onAction="#startRace1" text="Race1" />
      <Button fx:id="race2" layoutX="109.0" layoutY="55.0" mnemonicParsing="false" onAction="#startRace2" text="Race2" />
      <Button fx:id="race3" layoutX="109.0" layoutY="89.0" mnemonicParsing="false" onAction="#startRace3" text="Championship" />
      <Button fx:id="trainRun" layoutX="260.0" layoutY="21.0" mnemonicParsing="false" onAction="#startTrainingRun" text="Train running" />
      <Button fx:id="trainFly" layoutX="260.0" layoutY="55.0" mnemonicParsing="false" onAction="#startTrainingFly" text="Train flying" />
      <ImageView fx:id="duckHolder" fitHeight="80.0" fitWidth="48.0" layoutX="124.0" layoutY="309.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../duck.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
